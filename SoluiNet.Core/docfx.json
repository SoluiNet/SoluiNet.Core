{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "**.csproj"
          ],
          "src": "./"
        }
      ],
      "dest": "docs/SoluiNet.Core",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    },
    {
      "src": [
        {
          "files": [
            "**.csproj"
          ],
          "src": "../SoluiNet.Core.UI/"
        }
      ],
      "dest": "docs/SoluiNet.Core.UI",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    },
    {
      "src": [
        {
          "files": [
            "**.csproj"
          ],
          "src": "../SoluiNet.Core.UI.WPF/"
        }
      ],
      "dest": "docs/SoluiNet.Core.UI.WPF",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "docs/SoluiNet.Core/**.yml",
          "docs/SoluiNet.Core/index.md",
          "docs/SoluiNet.Core.UI/**.yml",
          "docs/SoluiNet.Core.UI/index.md",
          "docs/SoluiNet.Core.UI.WPF/**.yml",
          "docs/SoluiNet.Core.UI.WPF/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "../docs/output",
    "globalMetadata": {
      "_appTitle": "SoluiNet.Core",
      "_appLogoPath": "images/logo.svg",
      "_appFaviconPath": "images/favicon.ico",
      "_gitContribute": {
        "repo": "https://github.com/SoluiNet/SoluiNet.Core",
        "branch": "master",
        "apiSpecFolder": "SoluiNet.Core/apidoc"
      },
      "_enableSearch": "true"
    },
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "statictoc"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}