{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "**.csproj"
          ],
          "src": "./"
        }
      ],
      "dest": "docs/SoluiNet.Core/net461",
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "properties":
      {
        "TargetFramework": "net461"
      }
    },
    {
      "src": [
        {
          "files": [
            "**.csproj"
          ],
          "src": "./"
        }
      ],
      "dest": "docs/SoluiNet.Core/netstandard20",
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "properties":
      {
        "TargetFramework": "netstandard2.0"
      }
    },
    {
      "src": [
        {
          "files": [
            "**.csproj"
          ],
          "src": "../SoluiNet.Core.UI"
        }
      ],
      "dest": "docs/SoluiNet.Core.UI/net461",
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "properties":
      {
        "TargetFramework": "net461"
      }
    },
    {
      "src": [
        {
          "files": [
            "**.csproj"
          ],
          "src": "../SoluiNet.Core.UI"
        }
      ],
      "dest": "docs/SoluiNet.Core.UI/netstandard20",
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "properties":
      {
        "TargetFramework": "netstandard2.0"
      }
    },
    {
      "src": [
        {
          "files": [
            "**.csproj"
          ],
          "src": "../SoluiNet.Core.UI.WPF"
        }
      ],
      "dest": "docs/SoluiNet.Core.UI.WPF/net461",
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "properties":
      {
        "TargetFramework": "net461"
      }
    },
    {
      "src": [
        {
          "files": [
            "**.csproj"
          ],
          "src": "../SoluiNet.Core.UI.WPF"
        }
      ],
      "dest": "docs/SoluiNet.Core.UI.WPF/netstandard20",
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "properties":
      {
        "TargetFramework": "netstandard2.0"
      }
    }
  ],
  "merge": {
    "content":
    [
      {
        "files": "**.yml",
        "src": "docs/",
      }
    ],
    "fileMetadata": 
    {
      "platform":
      {
        "docs/**/netstandard20/**.yml": 
        [
          "netstandard2.0"
        ],
        "docs/**/net461/**.yml":
        [
          "net461"
        ]
      }
    },
    "dest": "docs/merged"
  },
  "build": {
    "content": [
      {
        "files": [
          "docs/merged/**.yml",
          "docs/**/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "../docs/output",
    "globalMetadata": {
      "_appTitle": "SoluiNet.Core",
      "_appLogoPath": "images/logo.svg",
      "_appFaviconPath": "images/favicon.ico",
      "_gitContribute": {
        "repo": "https://github.com/SoluiNet/SoluiNet.Core",
        "branch": "master",
        "apiSpecFolder": "SoluiNet.Core/apidoc"
      },
      "_enableSearch": "true"
    },
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "statictoc"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}